<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>GeneralLogin.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>EmailPro</servlet-name>
        <servlet-class>org.health.EmailPro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmailPro</servlet-name>
        <url-pattern>/EmailPro</url-pattern>
    </servlet-mapping>
    <!-- SMTP settings -->
    <context-param>
        <param-name>host</param-name>
        <param-value>smtp.gmail.com</param-value>
    </context-param>
    <context-param>
        <param-name>port</param-name>
        <param-value>587</param-value>
    </context-param>
    <context-param>
        <param-name>user</param-name>
        <param-value>kiirafirst@gmail.com</param-value>
    </context-param>
    <context-param>
        <param-name>pass</param-name>
        <param-value>P@ss1234</param-value>
    </context-param>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/customTag.tld</tag-uri>
            <tag-locationg>C:/Users/HP/Desktop/eVAT/web/WEB-INF/tlds/myTags.tld</tag-locationg>
        </taglib>
    </tag-config>
    
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/BookingLib.tld</tag-uri>
            <tag-locationg>C:/Users/HP/Desktop/eVAT/web/WEB-INF/tlds/BookingLib.tld</tag-locationg>
        </taglib>
    </tag-config>
    
    <servlet>
        <servlet-name>administration</servlet-name>
        <servlet-class>org.health.administration</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/authTag.tld</tag-uri>
            <tag-locationg>C:/Users/HP/Desktop/eVAT/web/WEB-INF/tlds/authTag.tld</tag-locationg>
        </taglib>
    </tag-config>
    
    
    
    <servlet>
        <servlet-name>loginAuths</servlet-name>
        <servlet-class>org.health.loginAuths</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientAuths</servlet-name>
        <servlet-class>org.health.PatientAuths</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientRegj</servlet-name>
        <servlet-class>org.health.PatientRegj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingServ</servlet-name>
        <servlet-class>org.health.BookingServ</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>administration</servlet-name>
        <url-pattern>/administration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginAuths</servlet-name>
        <url-pattern>/loginAuths</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientAuths</servlet-name>
        <url-pattern>/PatientAuths</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientRegj</servlet-name>
        <url-pattern>/PatientRegj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingServ</servlet-name>
        <url-pattern>/BookingServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
